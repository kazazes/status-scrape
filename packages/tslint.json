{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["prisma/src/**/*", "**/*.d.ts"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "prefer-template": {
      "options": "allow-single-concat"
    }
  }
}
