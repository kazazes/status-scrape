{
  "name": "status-scrape",
  "version": "1.0.0",
  "author": "Peter Kazazes",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run --parallel clean",
    "clean:modules": "rimraf packages/*/node_modules node_modules",
    "deploy:gae": "npm-run-all clean lint build && gcloud beta app deploy",
    "lint": "lerna run --parallel lint",
    "test": "lerna run --parallel test",
    "serve": "yarn workspace @status-scrape/admin run serve",
    "start": "yarn workspace @status-scrape/server run start",
    "watch:ts": "bin/build-ts.sh",
    "watch:webpack": "yarn workspace @status-scrape/admin run watch:webpack",
    "debug": "yarn workspace @status-scrape/server watch:debug"
  },
  "devDependencies": {
    "@types/node": "^10.11.1",
    "jest-junit": "^5.2.0",
    "lerna": "^3.4.0",
    "node-notifier-cli": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "jest-express": "^1.8.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "supertest": "^3.3.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "yarn": "^1.9",
    "node": "^8"
  }
}
